.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", communities_path 
      - if user_signed_in?
        %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_community_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_community_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Community
    .inner
      = form_for @community, :url => community_path(@community), :html => { :class => :form } do |f|  
        = render 'error' 
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
